white-circ = circle(60, "solid", "white")
orange-circ = circle(60, "solid", "orange")
blue-circ = circle(50, "solid", "blue")
green-circ = circle(40, "solid", "green")
red-circ = circle(30, "solid", "red")
pole = circle(10, "solid", "black")

empty-pole = overlay(pole, white-circ)

#her forteller vi med navn bak hver definisjon hvilken figur vi jobber med

#figure1
step-one1 = overlay(blue-circ, orange-circ)
step-two1 = overlay(green-circ, step-one1)
step-three1 = overlay(red-circ, step-two1)
step-four1 = overlay(pole, step-three1)

figure-one1 = step-four1
figure-two1 = empty-pole
figure-three1 = empty-pole

beside-one1 = beside(figure-one1, figure-two1)
beside-two1 = beside(beside-one1, figure-three1)


#figure2
step-one2 = overlay(blue-circ, orange-circ)
step-two2 = overlay(green-circ, step-one2)
step-three2 = overlay(pole, step-two2)
step-four2 = overlay(red-circ, white-circ)
step-five2 = overlay(pole, step-four2)

figure-one2 = step-three2
figure-two2 = step-five2
figure-three2 = empty-pole

beside-one2 = beside(figure-one2, figure-two2)
beside-two2 = beside(beside-one2, figure-three2)


#figure3
step-one3 = overlay(blue-circ, orange-circ)
